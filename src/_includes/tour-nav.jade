.border-bottom-gray
  .row
    .column.medium-12
      ul.inline-list.subnav

      - var menu = { 'Overview': '/tour', 'Features': '/features', 'Enterprise': '/enterprise' };
      
      ul.inline-list.subnav
        each val, key in menu
          li
            if selected === key
              a.subnav-link.uppercase.small-text.selected(href=val, title=key)= key
            else
              a.subnav-link.uppercase.small-text(href=val, title=key)= key
