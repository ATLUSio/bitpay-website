- var menu = { 'Integrated Solutions': '#integrated', 'Plugins': '#plugins', 'Point of Sale': '#pos', 'Billing': '#billing', 'Donations': '#donations', 'Code Libraries': '#code-libraries' };

.border-bottom-gray.show-for-large-up
  .row
    .column.medium-12
      ul.inline-list.subnav
        each val, key in menu
          li
            if integrationSelected === key
              a.subnav-link.uppercase.selected(href=val title=key target=_blank)= key
            else
              a.subnav-link.uppercase(href=val title=key)= key
